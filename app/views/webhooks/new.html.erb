<% if flash[:alert] %>
  <% flash[:alert].map do |alert| %>
        <div class="notice"><%= alert %></div>
  <% end %>
<% end %>

<%= form_for @webhook, url: {action: "create"} do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <%= f.label :zip_code %>
  <%= f.text_field :zip_code %>
  <%= f.submit %>
<% end %>